{
  "name": "playwright-redactor",
  "version": "1.0.0",
  "description": "Remove sensitive data from your Playwright trace files",
  "author": {
    "name": "Ryan Rosello"
  },
  "main": "build/src/cli.js",
  "files": [
    "build/src"
  ],
  "bin": {
    "playwright-redactor": "build/src/cli.js"
  },
  "license": "MIT",
  "keywords": [
    "redact",
    "report",
    "playwright",
    "typescript"
  ],
  "scripts": {
